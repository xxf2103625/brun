{
  "profiles": {
    "BrunSimple": {
      "commandName": "Project",
      "launchBrowser": true,
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "http://*:5000"
      },
      "applicationUrl": "http://localhost:5000",
      "dotnetRunMessages": true
